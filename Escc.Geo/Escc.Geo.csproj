<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Calculations and conversions for geographic data</Description>
    <Company>East Sussex County Council</Company>
    <Authors>East Sussex County Council</Authors>
    <Copyright>East Sussex County Council</Copyright>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/east-sussex-county-council/Escc.Geo</PackageProjectUrl>
    <RepositoryUrl>https://github.com/east-sussex-county-council/Escc.Geo</RepositoryUrl>
    <PackageIconUrl>https://www.eastsussex.gov.uk/apple-touch-icon.png</PackageIconUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>geo latitude longitude</PackageTags>
    <PackageReleaseNotes>1.2.0 targets .NET Standard</PackageReleaseNotes>
    <NeutralLanguage>en-GB</NeutralLanguage>
    <Version>1.2.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
